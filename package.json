{
  "name": "sbas-frontend-web",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "run-dev": "vite --port 80",
    "run-with-worker": "wrangler pages dev dist --port 80 --proxy 8080",
    "build-dev": "vite build --mode development && build-worker --entry ./public/_worker.js --out dist/_worker.js",
    "buld-worker": "build-worker -e .env.development --entry ./public/_worker.js --out dist/_worker.js --debug",
    "build": "vite build && build-worker --entry ./public/_worker.js --out dist/_worker.js",
    "deploy": "wrangler pages deploy dist --project-name=dev-sbas-frontend"
  },
  "dependencies": {
    "@vavt/md-editor-extension": "^3.0.0",
    "apexcharts": "^3.41.0",
    "axios": "^1.6.0",
    "babel-runtime": "^6.26.0",
    "core-js": "^3.8.3",
    "crypto-js": "^4.2.0",
    "fullpage.js": "^4.0.22",
    "vite-plugin-mkcert": "1.17.3",
    "jquery": "^3.7.0",
    "md-editor-v3": "^4.6.0",
    "mitt": "^3.0.0",
    "mobile-detect": "^1.4.5",
    "swiper": "^10.2.0",
    "v-wave": "^1.5.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.0",
    "vue-easy-lightbox": "^1.16.0",
    "vue3-apexcharts": "^1.4.4",
    "vue3-markdown-it": "^1.0.10",
    "vue3-websocket": "^1.0.11",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0",
    "yup": "^1.2.0"
  },
