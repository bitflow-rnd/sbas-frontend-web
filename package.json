    "@types/markdown-it": "^13.0.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "build-worker": "^0.2.2",
    "typescript": "~5.0.4",
    "eslint": "^7.32.0",
